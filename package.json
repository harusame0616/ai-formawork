{
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"cspell": "^9.2.2",
		"knip": "catalog:"
	},
	"packageManager": "pnpm@10.12.4",
	"scripts": {
		"validate:check": "biome check . && knip && cspell . && pnpm -r type:check",
		"validate:fix": "biome check --write . && knip --fix --allow-remove-files"
	}
}
